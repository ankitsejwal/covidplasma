<div class="form-group">
  <input
    class="form-control form-control-lg"
    placeholder="Enter name"
    name="name"
    type="text"
  />
</div>
<div class="form-group">
  <input
    class="form-control form-control-lg"
    placeholder="Enter age"
    name="age"
    type="text"
  />
</div>
<div class="form-group">
  <select class="form-control form-control-lg" name="gender" id="">
    <option value="" disabled selected>Select Gender</option>
    <option value="male">Male</option>
    <option value="female">Female</option>
  </select>
</div>
<div class="form-group">
  <select class="form-control form-control-lg" name="bloodGroup" id="">
    <option value="" disabled selected>Select Blood Group</option>
    <option value="A+">A+</option>
    <option value="A-">A-</option>
    <option value="B+">B+</option>
    <option value="B-">B-</option>
    <option value="AB+">AB+</option>
    <option value="AB-">AB+</option>
    <option value="O+">O+</option>
    <option value="O-">O-</option>
    <option value="notsure">not sure</option>
  </select>
  <small class="form-text text-muted">Choose not sure when unaware</small>
</div>
<div class="form-group">
  <input
    class="form-control form-control-lg"
    placeholder="Enter email"
    name="email"
    type="email"
  />
</div>
<div class="form-group">
  <input
    class="form-control form-control-lg"
    placeholder="Enter phone"
    name="phone"
    type="text"
  />
</div>
<div class="form-group" id="locationField">
  <input
    class="form-control form-control-lg"
    style="width: 100%"
    id="autocomplete"
    placeholder="Town / City / Village"
    onFocus="geolocate()"
    type="text"
  />
  <small class="form-text text-muted">No need to put complete address</small>
</div>
<div id="address">
  <div class="form-group">
    <input
      class="form-control form-control-lg"
      id="locality"
      name="locality"
      placeholder="Locality"
      disabled="true"
    />
  </div>
  <div class="form-group">
    <input
      class="form-control form-control-lg"
      placeholder="State"
      id="administrative_area_level_1"
      name="state"
      disabled="true"
    />
  </div>
  <div class="form-group">
    <input
      class="form-control"
      id="postal_code"
      name="zipcode"
      placeholder="Zip Code"
      disabled="true"
      type="hidden"
    />
  </div>
  <div class="form-group">
    <input
      class="form-control form-control-lg"
      id="country"
      name="country"
      placeholder="Country"
      disabled="true"
    />
  </div>
</div>
<button class="btn btn-lg btn-block btn-primary" type="submit">Submit</button>
<script src="js/googleAddressAutocomple.js"></script>
